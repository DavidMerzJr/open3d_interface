<?xml version="1.0"?>
<launch>

  <arg name="depth_image_topic"/>
  <arg name="color_image_topic"/>
  <arg name="camera_info_topic"/>
  <arg name="relative_frame"/>
  <arg name="tracking_frame"/>
  <arg name="image_directory"/>
  <arg name="pub_rate"/>

  <node pkg="open3d_interface" type="open3d_archive_player" name="open3d_archive_player" output="screen">
    <param name="depth_image_topic" type="string" value="$(arg depth_image_topic)"/>
    <param name="color_image_topic" type="string" value="$(arg color_image_topic)"/>
    <param name="camera_info_topic" type="string" value="$(arg camera_info_topic)"/>
    <param name="relative_frame" type="string" value="$(arg relative_frame)"/>
    <param name="tracking_frame" type="string" value="$(arg tracking_frame)"/>
    <param name="image_directory" type="string" value="$(arg image_directory)"/>
    <param name="pub_rate" type="double" value="$(arg pub_rate)"/>
  </node>

</launch>
